				loc.getXML(loc1);
				function loc1(response) 
					{
						var answer = response.responseXML.documentElement.getAttribute("answer");
						answer = answer.evalJSON(); 
					
						var r = []; 
						for( var i=answer.length-1 ; i >=0; i--) 
							{ 	
								
								r[i] = answer[i];
								var abc= answer[i];
								if(abc!= null){
								g_form.addOption('site',abc,abc);	
								}
								 
							} 
						
						


					}
   
}]]></script>
